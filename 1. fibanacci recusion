#include<stdio.h>
int fiba(int n){
	if(n<=1)
	return n;
	else
	return fiba(n-1)+fiba(n-2);
}
int main(){
	int n;
	printf("enter the value of n:");
	scanf("%d",&n);
	for(int i=0;i<n;i++){
		printf("%d ",fiba(i));
	}
}
/*Output:
enter the value of n:6
0 1 1 2 3 5
--------------------------------
Process exited after 1.12 seconds with return value 0
Press any key to continue . . .*/
